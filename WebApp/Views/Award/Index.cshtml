@model IEnumerable<WebApp.Models.AwardModel>

@{
    ViewBag.Title = "Index";
}

<h2>Награды</h2>

<p>
    @Html.ActionLink("Добавить награду", "Add")
</p>
<table class="table">
    <tr>
        <th>
            Название
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) 
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
    </tr>
}

</table>
